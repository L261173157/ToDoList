<Window x:Class="Component.Views.DictOperate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True" Width="460" Height="200">
    <Grid>
        <Button Content="导入文件" HorizontalAlignment="Left" Margin="310,55,0,0" VerticalAlignment="Top" Command="{Binding NewDictCmd}"/>
        <TextBox HorizontalAlignment="Left" Margin="70,55,0,0" Text="{Binding CsvFileName}" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="70,100,0,0" Text="{Binding Test}" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
